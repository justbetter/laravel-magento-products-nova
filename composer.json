{
    "name": "justbetter/laravel-magento-products-nova",
    "description": "Nova integration for justbetter/laravel-magento-products",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Boon",
            "email": "vincent@justbetter.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "justbetter/laravel-magento-products": "^1.2",
        "laravel/framework": "^11.0|^12.0",
        "laravel/nova": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "JustBetter\\MagentoProductsNova\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "JustBetter\\MagentoProductsNova\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "scripts": {
        "analyse": "phpstan --memory-limit=256M",
        "coverage": "XDEBUG_MODE=coverage php vendor/bin/pest --coverage --min=0",
        "style": "pint --test",
        "quality": [
            "@style",
            "@analyse"
        ]
    }
}
